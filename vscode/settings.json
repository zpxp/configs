{
    "debug.hideActionBar": true,
    "clang-format.executable": "/usr/bin/clang-format",
    "editor.fontFamily": "'Monaco',  'Droid Sans Fallback'",
    "editor.letterSpacing": 0,
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "debug.showInStatusBar": "never",
    "statusbarDebugger.priority": 999999999,

    "gitlens.settings.mode": "advanced",
	"gitlens.historyExplorer.enabled": false,
    "arduino.defaultBaudRate": 9600,
    "arduino.path": "/usr/share/arduino",
    

    "git.confirmSync": false,

    "cmake.configureOnOpen": true,

	"workbench.list.automaticKeyboardNavigation": false,
    "gitlens.views.compare.enabled": false,
	 "window.zoomLevel": 0,
	 

	 "debug.toolBarLocation": "hidden",
	 // "editor.snippetSuggestions": "top",
	 "terminal.integrated.shell.windows": "C:\\windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
	 "editor.minimap.enabled": false,
	 "workbench.editor.tabSizing": "shrink",
	 "typescript.tsserver.trace": "off",
	 "vtools.autoHideSideBar": true,
	 "vtools.autoHideDelay": 10,
	 "extensions.ignoreRecommendations": true,
	 "explorer.confirmDelete": false,
	 "telemetry.enableTelemetry": false,
	 "update.enableWindowsBackgroundUpdates": false,
	 "explorer.confirmDragAndDrop": false,
	 "typescript.tsserver.log": "off",
	 "color-highlight.markerType": "dot-before",
	 "color-highlight.markRuler": false,
	 "beautify.options": {
		"preserve-newlines": true
	 },
	 "editor.tabSize": 3,
	 "editor.insertSpaces": false,
	 "editor.detectIndentation": false,
	 "javascript.updateImportsOnFileMove.enabled": "never",
	 "gitlens.keymap": "none",
	 "gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": true,
		"suppressShowKeyBindingsNotice": true
	},
	 "gitlens.currentLine.format": "                                                 ${author}, ${agoOrDate} â€¢ ${message}",
	 "editor.multiCursorModifier": "alt",
	 "typescript.updateImportsOnFileMove.enabled": "never",
	 "window.newWindowDimensions": "maximized",
	 "workbench.enableExperiments": false,
	 "javascript.autoClosingTags": true,
	 "javascript.preferences.importModuleSpecifier": "non-relative",
	 "breadcrumbs.enabled": true,
	 "html.format.extraLiners": "head, body, table, /html",
	 "html.format.wrapAttributes": "preserve-aligned",
	 "html.format.wrapLineLength": 130,
	 "search.useGlobalIgnoreFiles": true,
	 "gitlens.views.fileHistory.enabled": true,
	"gitlens.views.lineHistory.enabled": true,
	 "gitlens.mode.active": "review",
	 "editor.dragAndDrop": false,
	 "html.format.endWithNewline": true,
	 "files.exclude": {
		"**/bin/": true,
		"**/node_modules/": true,
		"**/obj/": true
	 },
	"prettier.disableLanguages": [
		 "scss",
		 "css"
	],
	"editor.gotoLocation.multiple": "gotoAndPeek",

	"git.enableSmartCommit": true,
	"workbench.startupEditor": "welcomePage",
	"workbench.tree.indent": 18,
	"workbench.tree.renderIndentGuides": "none",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"debug.onTaskErrors": "showErrors",
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.mouseWheelScrollSensitivity": 2,
	"git.autofetch": true,
	"editor.cursorSurroundingLines": 10,
	"[cpp]": {
		"editor.defaultFormatter": "xaver.clang-format"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"files.associations": {
		"*.mxx": "cpp",
		"*.tsx": "typescriptreact"
	},
	"kite.showWelcomeNotificationOnStartup": false,
	"python.autoComplete.addBrackets": true,
	"python.formatting.autopep8Args": [
		"--max-line-length",
		"120"
	],
	"latex-workshop.view.pdf.viewer": "tab",
	"cSpell.userWords": [
		"ASPNET",
		"Citus",
		"Gitlab",
		"Kubernetes",
		"Postgresql",
		"Tensorflow",
		"pathing",
		"redi",
		"zolugo"
	],
	"csharp.semanticHighlighting.enabled": true,
	"editor.semanticHighlighting.enabled": true,
	"python.showStartPage": false,
	"python.formatting.provider": "black",
	"window.titleBarStyle": "custom",
	"[c]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"[scss]": {
		"editor.defaultFormatter": "michelemelluso.code-beautifier"
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"scm.alwaysShowActions": true,
	"[css]": {
		"editor.defaultFormatter": "michelemelluso.code-beautifier"
	},
	"update.mode": "manual",
}